<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="700" d:DesignHeight="950"
        x:Class="AvaloniaApplication2.MainWindow"
        Title="AvaloniaApplication2">
	<StackPanel Orientation="Vertical" MaxWidth ="800" MaxHeight ="1200">
		<TextBlock Text ="Enter Url(should enter full path including https)"/>
		<TextBox Text ="{Binding Url}"/>
		<Button Content="Say hi!" Click="GetArtistDataButton_Click"/>
		<TextBlock Text ="Artist Name"/>
		<TextBox Text ="{Binding ArtistName}"/>
		<TextBlock Text ="Playlist Title"/>
		<TextBox Text ="{Binding PlaylistTitle}"/>
		<ListBox Items="{Binding Songs}"/>
		

	</StackPanel>
</Window>
